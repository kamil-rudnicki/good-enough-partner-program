{
    "name": "kamil-rudnicki/easy-partner-program",
    "description": "Partner program management system",
    "type": "project",
    "require": {
        "php": "^8.1",
        "slim/slim": "^4.0",
        "slim/psr7": "^1.6",
        "php-di/php-di": "^7.0",
        "vlucas/phpdotenv": "^5.5",
        "doctrine/dbal": "^3.7",
        "phpmailer/phpmailer": "^6.8",
        "firebase/php-jwt": "^6.9",
        "chartmogul/chartmogul-php": "^5.0",
        "php-http/guzzle7-adapter": "^1.0",
        "http-interop/http-factory-guzzle": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Kamil Rudnicki",
            "email": "kamil.rudnicki@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
